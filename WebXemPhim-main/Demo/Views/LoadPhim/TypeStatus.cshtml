@model IEnumerable<Demo.Models.TrangThai>

<div class="card-body">
    <div class="row">
        @foreach (var types in Model)
        {
            <div class="col-12 col-md-12 col-xl-3 mb-3">
                <a href="@Url.Action("StatusForType","LoadPhim", new { id= types.id_trang_thai} )" class="dropdown-item" style="color: rgb(221, 147, 9)">
                    @types.ten_trang_thai
                </a>
            </div>
        }
    </div>
</div>


